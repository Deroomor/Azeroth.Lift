@{
    Layout = null;
    List<Lift.Model.MenuInfo> lst = (List<Lift.Model.MenuInfo>)this.ViewData["lstMenuInfo"];
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div> 
        @{ 
            foreach (var item in lst)
            {
                <p>@(item.Name)</p>
            }
        }
    </div>
</body>
</html>
